void vTask_PISCAS ( void *pvParameters)
{

	int modo_pisca = (int)pvParameters;

	Serial.println("Task Piscas a correr");

	// Verifica qual o pisca acionado e pisca
	for(;;)
	{

		switch (modo_pisca)
		{

		case 0:

			Serial.println("Pisca Fixo Esquerdo");



				digitalWrite(LED_LEFT, HIGH);

				vTaskDelay(250 / portTICK_PERIOD_MS);

				digitalWrite(LED_LEFT, LOW);

				vTaskDelay(250 / portTICK_PERIOD_MS);


			break;

		case 1:

			Serial.println("Pisca Fixo Direito");



				digitalWrite(LED_RIGHT, HIGH);

				vTaskDelay(250 / portTICK_PERIOD_MS);

				digitalWrite(LED_RIGHT, LOW);

				vTaskDelay(250 / portTICK_PERIOD_MS);


			break;

		case 2:

			Serial.println("Pisca Temporario Esquerdo");

			if(xSemaphoreTake(xCountingSemaphore, 100 ) == pdTRUE)
		   	{

				digitalWrite(LED_LEFT, HIGH);

				vTaskDelay(250 / portTICK_PERIOD_MS);

				digitalWrite(LED_LEFT, LOW);

				vTaskDelay(250 / portTICK_PERIOD_MS);
		   	}

			break;

		case 3:

			Serial.println("Pisca Temporario Direito");

			if(xSemaphoreTake(xCountingSemaphore, 100 ) == pdTRUE)
		   	{

				digitalWrite(LED_RIGHT, HIGH);

				vTaskDelay(250 / portTICK_PERIOD_MS);

				digitalWrite(LED_RIGHT, LOW);

				vTaskDelay(250 / portTICK_PERIOD_MS);
		   	}

			break;

		case 4:

			Serial.println("4 Piscas");


			digitalWrite(LED_RIGHT, HIGH);
			digitalWrite(LED_LEFT, HIGH);

			vTaskDelay(250 / portTICK_PERIOD_MS);

			digitalWrite(LED_RIGHT, LOW);
			digitalWrite(LED_LEFT, LOW);

			vTaskDelay(250 / portTICK_PERIOD_MS);

			break;

		default:

			 Serial.println("Nenhum Pisca");
			 vTaskDelay(1000 / portTICK_PERIOD_MS);

			 break;
		}

		vTaskDelay(250 / portTICK_PERIOD_MS);
	}
}


